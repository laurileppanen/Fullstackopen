blogsRouter.put('/:id', async (request, response, next) => {
    const body = request.body

    const blog = {
      likes: body.likes,
    }

    Blog.findByIdAndUpdate(request.params.id, blog, {new: true})
      .then(updatedBlog => {
        response.json(updatedBlog)
      })
      .catch(error => next(error))

  })